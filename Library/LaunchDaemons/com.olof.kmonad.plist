<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.olof.kmonad</string>
    <key>ProgramArguments</key>
    <array>
        <string>/Users/olof/.config/kmonad/start-kmonad.sh</string>
    </array>
    <key>RunAtLoad</key>
    <true/>
    <key>KeepAlive</key>
    <true/>
    <key>StandardErrorPath</key>
    <string>/var/log/kmonad.err</string>
    <key>StandardOutPath</key>
    <string>/var/log/kmonad.out</string>
</dict>
</plist>
